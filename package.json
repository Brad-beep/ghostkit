{
  "name": "ghostkit",
  "title": "Ghost Kit",
  "version": "2.12.1",
  "description": "Blocks collection and extensions for Gutenberg",
  "license": "GPL-2.0",
  "author": "nK <https://nkdev.info>",
  "scripts": {
    "build": "wpeg --build",
    "dev": "wpeg --build --watch",
    "production": "npm run make-pot && npm run make-json && wpeg --build --zip",
    "php-lint": "composer phpcs",
    "php-lint-fix": "composer phpcbf",
    "css-lint": "stylelint --syntax scss \"src/**/*.scss\"",
    "css-lint-fix": "stylelint --fix --syntax scss \"src/**/*.scss\"",
    "js-lint": "eslint --ext .js src/",
    "js-lint-fix": "eslint --ext .js --fix src/",
    "lint": "npm run php-lint && npm run css-lint && npm run js-lint",
    "lint-fix": "npm run php-lint-fix && npm run css-lint-fix && npm run js-lint-fix",
    "make-pot": "composer wp -- i18n make-pot ./src ./src/languages/ghostkit.pot --skip-audit --exclude=vendor,vendors,node_modules --headers='{\"Last-Translator\":\"nK\",\"Report-Msgid-Bugs-To\":\"https://github.com/nk-o/ghostkit/issues\"}'",
    "make-json": "po2json ./src/languages/ghostkit.pot ./src/languages/ghostkit.json --format=jed1.x --pretty --domain=@@text_domain",
    "update:packages": "npm i -g npm-check-updates && npm-check-updates -u && npm install",
    "postinstall": "composer install"
  },
  "devDependencies": {
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@svgr/webpack": "^5.3.1",
    "@wordpress/base-styles": "^1.6.0",
    "base64-inline-loader": "^1.1.1",
    "eslint-config-nk": "^0.0.15",
    "husky": "^3.1.0",
    "json-file": "^0.1.0",
    "lint-staged": "^10.1.3",
    "md5": "^2.2.1",
    "micromatch": "^4.0.2",
    "po2json": "^0.4.5",
    "stylelint-config-nk": "^0.0.8",
    "wpeg": "0.0.23"
  },
  "dependencies": {
    "brace": "^0.11.1",
    "classnames": "^2.2.6",
    "deep-equal": "^1.0.1",
    "qs": "^6.8.0",
    "rafl": "^1.2.2",
    "react-ace": "^6.3.2",
    "react-google-maps": "^9.4.5",
    "react-lazyload": "^2.6.2",
    "react-masonry-component": "^6.2.1",
    "react-select": "^3.0.4",
    "react-select-virtualized": "^2.0.19",
    "react-virtualized": "^9.21.1",
    "recompose": "^0.30.0",
    "scriptjs": "^2.5.9",
    "scrollreveal": "^4.0.5",
    "shorthash": "0.0.2",
    "slugify": "^1.3.4",
    "striptags": "^3.1.1",
    "throttle-debounce": "^2.0.1"
  }
}
